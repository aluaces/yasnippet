#!/usr/bin/make -f

build: build-stamp
build-stamp:
	emacs -Q -L . --batch -l doc/yas-doc-helper.el -f yas--generate-html-batch
	dh build
	touch build-stamp

clean:
	-rm doc/*html doc/html-revision
	dh clean

install: build install-stamp
install-stamp:
	dh install
	touch install-stamp

binary-arch: install
	dh binary-arch

binary-indep: install
	dh binary-indep

binary: binary-arch binary-indep
